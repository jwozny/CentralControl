<Window xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:Total_Control"
        x:Class="Total_Control.MainWindow"
        Title="Total Control" 
        Height="635" Width="1000" 
        FontFamily="Consolas" 
        Visibility="Visible" ResizeMode="CanMinimize" Activated="RestoreWindow" WindowStyle="None" 
        SnapsToDevicePixels="True">
    <Window.Resources>
        <DropShadowEffect x:Key="PrimaryGlow" ShadowDepth="0" Color="#FFFD3333" Opacity="1" BlurRadius="30"/>
    </Window.Resources>
    <Grid Background="#FF404040">
        <Grid.ColumnDefinitions>
